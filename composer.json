{
    "name": "ydg/meituan-union-sdk",
    "description": "meituan union sdk",
    "license": "MIT",
    "keywords": [
        "php",
        "sdk",
        "meituan",
        "meituan-union",
        "meituan-union-sdk",
        "meituan-cps"
    ],
    "require": {
        "php": "^7.0|^8.0",
        "ydg/foudation-sdk": "^1.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.12",
        "phpunit/phpunit": "^9.5",
        "vlucas/phpdotenv": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Ydg\\MeituanUnionSdk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "YdgTest\\MeituanUnionSdk\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "scripts": {
        "test": "phpunit",
        "cs-fix": "php-cs-fixer fix ."
    }
}